             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:administration="clr-namespace:Seq.Forwarder.Administration"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"></BooleanToVisibilityConverter>
        <administration:BooleanToBrushConverter x:Key="BooleanToBrushConverter"></administration:BooleanToBrushConverter>
        <administration:BooleanToButtonBrushConverter x:Key="BooleanToButtonBrushConverter"></administration:BooleanToButtonBrushConverter>
